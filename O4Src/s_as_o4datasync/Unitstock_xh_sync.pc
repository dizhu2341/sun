            int asset_id[BATCH_ROWS];//                        number(10,0)   DEFAULT 0       NOT NULL   , 
            int combi_id[BATCH_ROWS];//                        number(10,0)   DEFAULT 0       NOT NULL   , 
            char invest_type[BATCH_ROWS][2];//                     char(1)        DEFAULT '' ''   NOT NULL   , 
            char position_type[BATCH_ROWS][2];//                   char(1)        DEFAULT '' ''   NOT NULL   , 
            char stockholder_id[BATCH_ROWS][21];//                  varchar2(20)   DEFAULT '' ''   NOT NULL   , 
            char bind_seat[BATCH_ROWS][21];//                       varchar2(20)   DEFAULT '' ''   NOT NULL   , 
            int market_no[BATCH_ROWS];//                       number(8,0)    DEFAULT 0       NOT NULL   , 
            char report_code[BATCH_ROWS][33];//                     varchar2(32)   DEFAULT '' ''   NOT NULL   , 
            char position_stock_type[BATCH_ROWS][2];//            char(1)        DEFAULT '' ''   NOT NULL   , 
            double current_amount[BATCH_ROWS];//                  number(20,4)   DEFAULT 0       NOT NULL   , 
            double original_cost[BATCH_ROWS];//                   number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_cost[BATCH_ROWS];//                  number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_real_cost[BATCH_ROWS];//              number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_real_cost[BATCH_ROWS];//             number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_interest_cost[BATCH_ROWS];//          number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_interest_cost[BATCH_ROWS];//         number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_profit[BATCH_ROWS];//                 number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_profit[BATCH_ROWS];//                number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_real_profit[BATCH_ROWS];//            number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_real_profit[BATCH_ROWS];//           number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_interest_profit[BATCH_ROWS];//        number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_interest_profit[BATCH_ROWS];//       number(18,2)   DEFAULT 0       NOT NULL   , 
            double dividend[BATCH_ROWS];//                        number(18,2)   DEFAULT 0       NOT NULL   , 
            double uncarryover_dividend[BATCH_ROWS];//            number(18,2)   DEFAULT 0       NOT NULL   , 
            double ontheway_dividend[BATCH_ROWS];//               number(18,2)   DEFAULT 0       NOT NULL   , 
            double total_buy_amount[BATCH_ROWS];//                number(20,4)   DEFAULT 0       NOT NULL   , 
            double total_sale_amount[BATCH_ROWS];//               number(20,4)   DEFAULT 0       NOT NULL   , 
            double total_buy_balance[BATCH_ROWS];//               number(18,2)   DEFAULT 0       NOT NULL   , 
            double total_sale_balance[BATCH_ROWS];//              number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_cost_trade[BATCH_ROWS];//             number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_cost_trade[BATCH_ROWS];//            number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_profit_trade[BATCH_ROWS];//           number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_profit_trade[BATCH_ROWS];//          number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_real_cost_trade[BATCH_ROWS];//        number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_real_cost_trade[BATCH_ROWS];//       number(18,2)   DEFAULT 0       NOT NULL   , 
            double original_real_profit_trade[BATCH_ROWS];//      number(18,2)   DEFAULT 0       NOT NULL   , 
            double carryover_real_profit_trade[BATCH_ROWS];//     number(18,2)   DEFAULT 0       NOT NULL   , 
            double combi_amount[BATCH_ROWS];//                    number(20,4)   DEFAULT 0       NOT NULL   , 
            double buy_amount[BATCH_ROWS];//                      number(20,4)   DEFAULT 0       NOT NULL   , 
            double sale_amount[BATCH_ROWS];//                     number(20,4)   DEFAULT 0       NOT NULL   , 
            double buy_balance[BATCH_ROWS];//                     number(18,2)   DEFAULT 0       NOT NULL   , 
            double sale_balance[BATCH_ROWS];//                    number(18,2)   DEFAULT 0       NOT NULL   , 
            double buy_fee[BATCH_ROWS];//                         number(18,2)   DEFAULT 0       NOT NULL   , 
            double sale_fee[BATCH_ROWS];//                        number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_original_real_cost[BATCH_ROWS];//        number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_carryover_real_cost[BATCH_ROWS];//       number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_original_real_profit[BATCH_ROWS];//      number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_carryover_real_profit[BATCH_ROWS];//     number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_original_interest_cost[BATCH_ROWS];//    number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_carryover_interest_cost[BATCH_ROWS];//   number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_original_int_profit[BATCH_ROWS];//       number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_carryover_int_profit[BATCH_ROWS];//      number(18,2)   DEFAULT 0       NOT NULL   , 
            double begin_amount[BATCH_ROWS];//                    number(20,4)   DEFAULT 0       NOT NULL     
        } UnitStockXhSyncRecord;
        UnitStockXhSyncRecord * lpUnitStockXhSyncRecord = NULL;
    EXEC SQL END DECLARE SECTION;
    
    // 获取O3数据库连接句柄
    sql_context O3DbContext = m_lpO3Conn->getSQLContext(); 
    if (NULL == O3DbContext)
    {
        SetErrorInfo(DB_ERR_CONNECTION, "【%s】表同步，获取O3数据库连接失败", m_sDstTable);
        return false;
    }
    // 从O3视图读取数据放入到游标
    LOGDETAIL("【" << m_sDstTable << "】表同步，查询O3View【" << m_sSrcView << "】数据开始");
    EXEC SQL CONTEXT USE :O3DbContext;
    EXEC SQL DECLARE RESULT CURSOR FOR 
        select
            business_date, 
            company_id, 
            fund_id, 
            asset_id, 
            combi_id, 
            invest_type, 
            position_type, 
            stockholder_id, 
            bind_seat, 
            market_no, 
            report_code, 
            position_stock_type, 
            current_amount, 
            original_cost, 
            carryover_cost, 
            original_real_cost, 
            carryover_real_cost, 
            original_interest_cost, 
            carryover_interest_cost, 
            original_profit, 
            carryover_profit, 
            original_real_profit, 
            carryover_real_profit, 
            original_interest_profit, 
            carryover_interest_profit, 
            dividend, 
            uncarryover_dividend, 
            ontheway_dividend, 
            total_buy_amount, 
            total_sale_amount, 
            total_buy_balance, 
            total_sale_balance, 
            original_cost_trade, 
            carryover_cost_trade, 
            original_profit_trade, 
            carryover_profit_trade, 
            original_real_cost_trade, 
            carryover_real_cost_trade, 
            original_real_profit_trade, 
            carryover_real_profit_trade, 
            combi_amount, 
            buy_amount, 
            sale_amount, 
            buy_balance, 
            sale_balance, 
            buy_fee, 
            sale_fee, 
            begin_original_real_cost, 
            begin_carryover_real_cost, 
            begin_original_real_profit, 
            begin_carryover_real_profit, 
            begin_original_interest_cost, 
            begin_carryover_interest_cost, 
            begin_original_int_profit, 
            begin_carryover_int_profit, 
            begin_amount
        from vo4jc_mktunitstock_redo;
    
    // 打开游标
    EXEC SQL OPEN RESULT;
    if (sqlca.sqlcode < 0)
    {
        SetErrorInfo(SYNC_ERR_QUERY_SRC_TABLE, "【%s】表同步，查询O3View【%s】数据失败【%s】", m_sDstTable, m_sSrcView, sqlca.sqlerrm.sqlerrmc);
        return false;
    }
    LOGDETAIL("【" << m_sDstTable << "】表同步，查询O3View【" << m_sSrcView << "】完成，总行数【" << m_nTotalRows << "】条");
    
    // 申请内存空间，把游标中的数据导入到内存空间
    lpUnitStockXhSyncRecord =  new UnitStockXhSyncRecord();
    if (NULL == lpUnitStockXhSyncRecord)
    {
        SetErrorInfo(SYNC_ERR_MALLOC_MEMORY, "【%s】表同步，申请结构体内存【StockbusinessRecord】失败", m_sDstTable);
        return false;
    }
    
    // 获取O4数据连接句柄
    sql_context O4DbContext = m_lpO4Conn->getSQLContext();
    if (NULL == O4DbContext)
    {
        SetErrorInfo(DB_ERR_CONNECTION, "【%s】表同步，获取O4数据库连接失败", m_sDstTable);
        delete lpUnitStockXhSyncRecord;
        return false;
    }
    for (int i = 0; i < m_nLoopTimes; ++i)
    {
        // 清空内存，并把游标里的结果集放入内存中，一次放入BATCH_ROWS行数据
        memset(lpUnitStockXhSyncRecord, 0, sizeof (UnitStockXhSyncRecord));
        EXEC SQL CONTEXT USE :O3DbContext;
        EXEC SQL FETCH RESULT into :lpUnitStockXhSyncRecord;
        if (sqlca.sqlcode < 0)
        {
            SetErrorInfo(SYNC_ERR_EXPORT_DATA_TEMP, "【%s】表同步，导入数据到中间转换表【%s】时FETCH数据失败【%s】", m_sDstTable, m_sTmpTable, sqlca.sqlerrm.sqlerrmc);
            delete lpUnitStockXhSyncRecord;
            return false;
        }
        
        // 如果是最后一次循环，那么nFetchRows就等于m_nRemainRows行数据，m_nRemainRows小于等于BATCH_ROWS
        // 如果不是最后一次循环，那么nFetchRows就等于m_nBatchRows=BATCH_ROWS行数据
        nFetchRows = (i == (m_nLoopTimes - 1)) ? m_nRemainRows : m_nBatchRows;
        
        // 把内存里的数据导入到中间表
        EXEC SQL CONTEXT USE :O4DbContext;
        EXEC SQL for :nFetchRows
            insert into sync_unitstock_xh_sync(
                business_date, 
                company_id, 
                fund_id, 
                asset_id, 
                combi_id, 
                invest_type, 
                position_type, 
                stockholder_id, 
                bind_seat, 
                market_no, 
                report_code, 
                position_stock_type, 
                current_amount, 
                original_cost, 
                carryover_cost, 
                original_real_cost, 
                carryover_real_cost, 
                original_interest_cost, 
                carryover_interest_cost, 
                original_profit, 
                carryover_profit, 
                original_real_profit, 
                carryover_real_profit, 
                original_interest_profit, 
                carryover_interest_profit, 
                dividend, 
                uncarryover_dividend, 
                ontheway_dividend, 
                total_buy_amount, 
                total_sale_amount, 
                total_buy_balance, 
                total_sale_balance, 
                original_cost_trade, 
                carryover_cost_trade, 
                original_profit_trade, 
                carryover_profit_trade, 
                original_real_cost_trade, 
                carryover_real_cost_trade, 
                original_real_profit_trade, 
                carryover_real_profit_trade, 
                combi_amount, 
                buy_amount, 
                sale_amount, 
                buy_balance, 
                sale_balance, 
                buy_fee, 
                sale_fee, 
                begin_original_real_cost, 
                begin_carryover_real_cost, 
                begin_original_real_profit, 
                begin_carryover_real_profit, 
                begin_original_interest_cost, 
                begin_carryover_interest_cost, 
                begin_original_int_profit, 
                begin_carryover_int_profit, 
                begin_amount)
            values(
                :lpUnitStockXhSyncRecord->business_date, 
                :lpUnitStockXhSyncRecord->company_id, 
                :lpUnitStockXhSyncRecord->fund_id, 
                :lpUnitStockXhSyncRecord->asset_id, 
                :lpUnitStockXhSyncRecord->combi_id, 
                rtrim(:lpUnitStockXhSyncRecord->invest_type), 
                rtrim(:lpUnitStockXhSyncRecord->position_type), 
                rtrim(:lpUnitStockXhSyncRecord->stockholder_id), 
                rtrim(:lpUnitStockXhSyncRecord->bind_seat), 
                :lpUnitStockXhSyncRecord->market_no, 
                rtrim(:lpUnitStockXhSyncRecord->report_code), 
                rtrim(:lpUnitStockXhSyncRecord->position_stock_type), 
                :lpUnitStockXhSyncRecord->current_amount, 
                :lpUnitStockXhSyncRecord->original_cost, 
                :lpUnitStockXhSyncRecord->carryover_cost, 
                :lpUnitStockXhSyncRecord->original_real_cost, 
                :lpUnitStockXhSyncRecord->carryover_real_cost, 
                :lpUnitStockXhSyncRecord->original_interest_cost, 
                :lpUnitStockXhSyncRecord->carryover_interest_cost, 
                :lpUnitStockXhSyncRecord->original_profit, 
                :lpUnitStockXhSyncRecord->carryover_profit, 
                :lpUnitStockXhSyncRecord->original_real_profit, 
                :lpUnitStockXhSyncRecord->carryover_real_profit, 
                :lpUnitStockXhSyncRecord->original_interest_profit, 
                :lpUnitStockXhSyncRecord->carryover_interest_profit, 
                :lpUnitStockXhSyncRecord->dividend, 
                :lpUnitStockXhSyncRecord->uncarryover_dividend, 
                :lpUnitStockXhSyncRecord->ontheway_dividend, 
                :lpUnitStockXhSyncRecord->total_buy_amount, 
                :lpUnitStockXhSyncRecord->total_sale_amount, 
                :lpUnitStockXhSyncRecord->total_buy_balance, 
                :lpUnitStockXhSyncRecord->total_sale_balance, 
                :lpUnitStockXhSyncRecord->original_cost_trade, 
                :lpUnitStockXhSyncRecord->carryover_cost_trade, 
                :lpUnitStockXhSyncRecord->original_profit_trade, 
                :lpUnitStockXhSyncRecord->carryover_profit_trade, 
                :lpUnitStockXhSyncRecord->original_real_cost_trade, 
                :lpUnitStockXhSyncRecord->carryover_real_cost_trade, 
                :lpUnitStockXhSyncRecord->original_real_profit_trade, 
                :lpUnitStockXhSyncRecord->carryover_real_profit_trade, 
                :lpUnitStockXhSyncRecord->combi_amount, 
                :lpUnitStockXhSyncRecord->buy_amount, 
                :lpUnitStockXhSyncRecord->sale_amount, 
                :lpUnitStockXhSyncRecord->buy_balance, 
                :lpUnitStockXhSyncRecord->sale_balance, 
                :lpUnitStockXhSyncRecord->buy_fee, 
                :lpUnitStockXhSyncRecord->sale_fee, 
                :lpUnitStockXhSyncRecord->begin_original_real_cost, 
                :lpUnitStockXhSyncRecord->begin_carryover_real_cost, 
                :lpUnitStockXhSyncRecord->begin_original_real_profit, 
                :lpUnitStockXhSyncRecord->begin_carryover_real_profit, 
                :lpUnitStockXhSyncRecord->begin_original_interest_cost, 
                :lpUnitStockXhSyncRecord->begin_carryover_interest_cost, 
                :lpUnitStockXhSyncRecord->begin_original_int_profit, 
                :lpUnitStockXhSyncRecord->begin_carryover_int_profit, 
                :lpUnitStockXhSyncRecord->begin_amount);
        if (sqlca.sqlcode < 0)
        {
            SetErrorInfo(DB_ERR_INSERT, "【%s】表同步，导入数据到中间转换表【%s】失败【%s】", m_sDstTable, m_sTmpTable, sqlca.sqlerrm.sqlerrmc);
            delete lpUnitStockXhSyncRecord;
            return false;
        }
        EXEC SQL COMMIT;
    }
    
    EXEC SQL CONTEXT USE :O3DbContext;
    // 关闭游标
    EXEC SQL CLOSE RESULT;
    delete lpUnitStockXhSyncRecord;
    
    LOGDETAIL("【" << m_sDstTable << "】表同步，导入数据到中间转换表【" << m_sTmpTable << "】完成");
    return true;
}

