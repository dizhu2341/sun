CUnitStockXhSync::CUnitStockXhSync(IAS2Context * lpContext) : CDataSync(lpContext)
{
    strcpy(m_sSrcTable, "tunitstock");
    strcpy(m_sSrcView,  "vo4jc_mktunitstock_redo");
    strcpy(m_sTmpTable, "sync_unitstock_xh_sync");
    strcpy(m_sDstTable, "uft_tunitstock_xh_sync");
    m_nBatchRows = BATCH_ROWS;
    EXEC SQL WHENEVER SQLERROR DO SqlErrorAction();
}

CUnitStockXhSync::~CUnitStockXhSync()
{}

bool CUnitStockXhSync::ExpO3View2Temp()
{
    LOGDETAIL("【" << m_sDstTable << "】表同步，导入数据到中间转换表【" << m_sTmpTable << "】开始");

    struct sqlca sqlca;
    EXEC SQL ENABLE THREADS;
    
    EXEC SQL BEGIN DECLARE SECTION;
        int nFetchRows;
       
        typedef struct UnitStockXhSyncRecord
        {
            int business_date[BATCH_ROWS];//                   number(8,0)    DEFAULT 0       NOT NULL   , 
